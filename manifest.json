{
    "name": "Application SpreadSheet Logger",
    "version": "1.0",
    "description": "Helping to log applications easier",
    "permissions": ["activeTab", "declarativeContent", "storage", "background", 
                    "<all_urls>"], 
    "background": {
        "scripts": ["background.js"],
        "persistent": true 
    }, 
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/spread16.png",
            "32": "images/spread32.png",
            "48": "images/spread48.png",
            "128": "images/spread128.png"
          }
    },
    "icons": {
    "16": "images/spread16.png",
    "32": "images/spread32.png",
    "48": "images/spread48.png",
    "128": "images/spread128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}